{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./src/",
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "./node_modules/braintree-jsdoc-template",
        "encoding": "utf8",
        "destination": "./docs/_html/",
        "tutorials": "./docs/tutorials/",
        "readme": "./README.md",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "referenceTitle": "JS Package",
        "disableSort": false,
        "collapse": true
    }
}
