{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
    	"no-param-reassign": "off",
      "no-shadow": "off",
      "prefer-destructuring": "off",
      "class-methods-use-this": "off",
      "import/no-named-as-default": "off",
      "no-else-return": "off",
      "padded-blocks": "off",
      "func-names": "off"
    }
}
